{
  "name": "thx-portal-rule",
  "version": "0.0.1-alpha.2",
  "description": "MM 可视化规则编辑器",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "========== dev ==========": "",
    "dev": "npm-run-all -p dev:tsc dev:umd",
    "dev:tsc": "tsc --watch --pretty --preserveWatchOutput --project tsconfig.json",
    "dev:umd": "thx-scripts dev",
    "========== build ==========": "",
    "build": "rm -fr build/*; npm-run-all -p build:tsc build:umd",
    "build:tsc": "tsc --listEmittedFiles --project tsconfig.json",
    "build:umd": "thx-scripts build",
    "========== examples ==========": "",
    "examples:start": "npm-run-all --parallel start:react-*",
    "start:react-guide-app": "cd examples/react-guide-app && yarn install && yarn start;",
    "========== link ==========": "",
    "examples:link": "yarn link; npm-run-all --parallel link:*",
    "link:react-guide-app": "cd examples/react-guide-app && yarn link thx-portal-rule",
    "========== release ==========": "",
    "release": "standard-version",
    "alpha": "npm run release -- --release-as prerelease",
    "contributors": "ali-contributors",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "========== release tnpm ==========": "",
    "daily:def": "def publish --daily",
    "publish:def": "def publish --prod; yarn run build; tnpm publish;",
    "publish:tnpm": "npm version patch; git add package.json; git commit -m 'publish: tnpm'; yarn run build; tnpm publish;"
  },
  "dependencies": {
    "@alifd/next": "^1.25.45",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.25",
    "npm-run-all": "^4.1.5",
    "standard-version": "^9.5.0",
    "thx-scripts": "^0.0.1-alpha.2"
  },
  "license": "MIT",
  "files": [
    "build",
    "src",
    ".rmxrc",
    "abc.json",
    "CHANGELOG.md",
    "package.json",
    "README.md",
    "tsconfig.json",
    "yarn.lock"
  ]
}
